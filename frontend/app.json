{
  "expo": {
    "name": "SR_Shield_CSC",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E6F4FE"
    },
    "android": {
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#E6F4FE"
      },
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "predictiveBackGestureEnabled": false,
      "package": "com.aetherionx.frontend",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "output": "single",
      "build": {
        "babel": {
          "dangerouslyAllowEval": true
        }
      },
      "icon": "./assets/images/icon.png",
      "bundler": "metro",
      "favicon": "./assets/images/web.png"
    },
    "plugins": [
      "expo-splash-screen",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload images."
        }
      ]
    ],
    "experiments": {},
    "extra": {
      "eas": {
        "projectId": "db23e781-beac-437f-89a3-a319ab4de4b7"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/db23e781-beac-437f-89a3-a319ab4de4b7"
    }
  }
}
